{
  "presets": [["@babel/preset-env",
    {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    ["babel-plugin-dotenv", {
      "replacedModuleName": "babel-dotenv"
    }]
  ],
  "sourceMaps": "both",
  "ignore": ["dist/*.test.js"]
}
